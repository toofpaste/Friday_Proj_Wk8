<h1>Salon Stylists</h1>
<p>Click on the stylist's name for client list</p>

@if (@Model.Count == 0)
{
  <h3>No Stylists have been added yet!</h3>
}

@foreach (var category in Model)
{
  <h3>Stylist: <a href='/categories/@category.GetId()'>@category.GetName()</a></h3>
}

<h3><a href='/categories/new'>Add a new Stylist</a></h3>
<h3><a href='/'>Back home</a></h3>